<outtype: win64>
// EXPECT: int64: 9223372036854775805\nfloat64: 2.246914

data {
    string fmt_i64 : "int64: %lld\n"
    string fmt_f64 : "float64: %f\n"
}

define printf(msg: string, all: ...args) -> int32;

export main(void) -> int32 {
    int64 i = 9223372036854775807;
    int64 j = 2;
    int64 k = int64(i - j);
    call printf(string fmt_i64, int64 k);

    float64 x = 1.123456789;
    float64 y = 2.0;
    float64 z = float64(x * y);
    call printf(string fmt_f64, float64 z);

    return int32 0;
}
