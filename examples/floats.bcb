<outtype: win64>
// EXPECT: a + b = 3.000000

data {
    string msg : "a + b = %f\n"
}

define printf(msg: string, all: ...args) -> int32;

export main(void) -> int32 {
    float32 a = 1.0;
    float32 b = 2.0;
    float32 c = float32(a + b); // using type(math) tells the compiler the type of the expression,
                                // because if not used, it compiles to int32 by default.
    call printf(string msg, float32 c);
    return int32 0;
}